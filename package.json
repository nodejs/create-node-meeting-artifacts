{
  "name": "create-node-meeting-artifacts",
  "version": "0.0.1",
  "description": "Creates issue and minutes doc for node meeting",
  "type": "module",
  "main": "create-node-meeting-artifacts.mjs",
  "bin": {
    "create-node-meeting-artifacts": "./create-node-meeting-artifacts.mjs"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@hackmd/api": "^2.5.0",
    "@octokit/rest": "^22.0.0",
    "commander": "^14.0.1",
    "dedent": "^1.6.0",
    "dotenv": "^17.2.2",
    "ical": "^0.8.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/ical": "^0.8.3",
    "@types/properties-parser": "^0.3.3",
    "eslint": "^9.33.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsdoc": "^54.0.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2"
  },
  "scripts": {
    "dev": "node --env-file=.env create-node-meeting-artifacts.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "npm run lint && npm run format:check",
    "test": "node --test test/*.test.mjs test/**/*.test.mjs",
    "test:watch": "node --test --watch test/*.test.mjs test/**/*.test.mjs",
    "test:coverage": "node --test --experimental-test-coverage test/*.test.mjs test/**/*.test.mjs"
